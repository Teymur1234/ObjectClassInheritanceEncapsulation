class cola{
    #hesab=0
    #colaqiymet=5
    #alinmadi=0
    moneytransfer(){
        return console.log("kocurulen pul -  " ,this.#hesab+=this.money);
    }
    say(){
        let qiymet=this.count*this.#colaqiymet
        if(this.#hesab<qiymet){
            console.log("hesab yetersizdir");
            this.#alinmadi++
        }
        else{
        return console.log("alinan kola sayi - "+this.count);
    }
    }
    qaliq(){
        if (this.#alinmadi===0) {
           return console.log("qaliq pul - ",this.#hesab-this.#colaqiymet*this.count);

        }
        else{
            console.log("cola alinmadi");
        }
    }
    constructor(pul, say){
        this.money=pul  
        this.count=say
    }
}
const cola1=new cola(20,5)
 cola1.moneytransfer()
cola1.say()
cola1.qaliq()
